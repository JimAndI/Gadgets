function Holer(pt, ang, dst, dia, lay)
  local group = ContourGroup(true)
  group:AddTail(CreateCircle(pt.x,pt.y,dia,0.0,0.0))
  pt = Polar2D(pt, ang, dst)
  group:AddTail(CreateCircle(pt.x,pt.y,dia,0.0,0.0))
  AddGroupToJob(group, lay)
  return true
end  --  function end
