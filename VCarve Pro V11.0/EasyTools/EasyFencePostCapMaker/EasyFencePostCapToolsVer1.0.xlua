function DrawCap()
  PostCap.pt1 = Point2D(0.0, 0.0)
  PostCap.pt2 = Point2D(PostCap.CapSizeX + (Milling.MillTool3.ToolDia * 2.0), 0.0)
  PostCap.pt3 = Point2D(PostCap.CapSizeX + (Milling.MillTool3.ToolDia * 2.0), PostCap.CapSizeY + (Milling.MillTool3.ToolDia * 2.0))
  PostCap.pt4 = Point2D(0.0, PostCap.CapSizeY + (Milling.MillTool3.ToolDia * 2.0))
  DrawBox(PostCap.pt1, PostCap.pt2, PostCap.pt3, PostCap.pt4, PostCap.Clearing)

  PostCap.pt1 = Point2D(Milling.MillTool3.ToolDia, Milling.MillTool3.ToolDia)
  PostCap.pt2 = Polar2D(PostCap.pt1,  0.0, PostCap.CapSizeX)
  PostCap.pt3 = Polar2D(PostCap.pt2, 90.0, PostCap.CapSizeY)
  PostCap.pt4 = Polar2D(PostCap.pt1, 90.0, PostCap.CapSizeY)
  DrawBox(PostCap.pt1, PostCap.pt2, PostCap.pt3, PostCap.pt4, PostCap.Border)
  DrawBox(PostCap.pt1, PostCap.pt2, PostCap.pt3, PostCap.pt4, PostCap.Clearing)

  local pt1, pt2
  pt1 = Polar2D(PostCap.pt1, 180.0, Milling.MillTool2.Stepover)
  pt1 = Polar2D(pt1,   90.0, PostCap.DropAmount)
  pt2 = Polar2D(pt1, 270.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))
  DrawLine(pt1, pt2,  PostCap.Carving)

  for i = PostCap.BottomCount,0, -1 do
    pt1 = Polar2D(pt1,   0.0, Milling.MillTool2.Stepover)
    pt2 = Polar2D(pt1, 270.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))
    DrawLine(pt1, pt2,  PostCap.Carving)
  end

  pt1 = Polar2D(PostCap.pt4, 180.0, Milling.MillTool2.Stepover)
  pt1 = Polar2D(pt1,  270.0, PostCap.DropAmount)
  pt2 = Polar2D(pt1,   90.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))

  DrawLine(pt1, pt2,  PostCap.Carving)

  for i = PostCap.BottomCount,0, -1 do
    pt1 = Polar2D(pt1,  0.0, Milling.MillTool2.Stepover)
    pt2 = Polar2D(pt1, 90.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))
    DrawLine(pt1, pt2,  PostCap.Carving)
  end

  pt1 = Polar2D(PostCap.pt2, 270.0, Milling.MillTool2.Stepover)
  pt1 = Polar2D(pt1, 180.0, PostCap.DropAmount)
  pt2 = Polar2D(pt1,   0.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))

  DrawLine(pt1, pt2,  PostCap.Carving)

  for i = PostCap.SideCount,0, -1 do
    pt1 = Polar2D(pt1, 90.0, Milling.MillTool2.Stepover)
    pt2 = Polar2D(pt1,  0.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))
    DrawLine(pt1, pt2,  PostCap.Carving)
  end

  pt1 = Polar2D(PostCap.pt4, 90.0, Milling.MillTool2.Stepover)
  pt1 = Polar2D(pt1, 0.0, PostCap.DropAmount)
  pt2 = Polar2D(pt1, 180.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))

  DrawLine(pt1, pt2,  PostCap.Carving)

  for i = PostCap.SideCount,0, -1 do
    pt1 = Polar2D(pt1, 270.0, Milling.MillTool2.Stepover)
    pt2 = Polar2D(pt1, 180.0, PostCap.DropAmount + (Milling.MillTool3.ToolDia * 0.5))
    DrawLine(pt1, pt2,  PostCap.Carving)
  end
  return true

end -- function end
