function CheckDialogValues()                          -- Look at what the User inputed.
  PostCap.DialogLoop = false
 if Milling.MillTool1 == "No Tool Selected"  then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: No Profile Tool Selected")
  elseif Milling.MillTool2 == "No Tool Selected"  then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: No Carving Tool Selected")
  elseif Milling.MillTool3 == "No Tool Selected"  then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: No Clearing Tool Selected")
  elseif PostCap.SlopAmount == 0.0 then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Slope Amount cannot be Zero")
  elseif PostCap.SlopAmount >= PostCap.MaterialThickness then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Slope Amount cannot be more or equal to the Material Thickness")
  elseif PostCap.FlatAmount >= PostCap.MaterialWidth then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Flat Amount cannot be more or equal to the Material Width")
  elseif PostCap.FlatAmount < 0.0 then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Flat Amount cannot be less than zero")
  elseif PostCap.FlatAmount >= PostCap.MaterialLength then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Flat Amount cannot be more or equal to the Material Length")
  elseif PostCap.FlatAmount >= PostCap.MaterialLength then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Flat Amount cannot be more or equal to the Material Length")
  elseif PostCap.CapSizeY == 0.0 then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Cap Size Y cannot be Zero")
  elseif PostCap.CapSizeX == 0.0 then
    PostCap.DialogLoop = true
    DisplayMessageBox("Error: The Cap Size X cannot be Zero")
  end
  return true
end -- function end
