function ExportWriter(xFile)
  -- Writes new xText Line to a log file
  local LogName = Project.ProjectPath .. "/" .. xFile .. "-" .. StartDateTime(false) .. ".ini"
  local fileW = io.open(LogName,  "w")
  fileW:write("[Default Settings]\n")
  fileW:write("BaseDim.CabDepth = " .. ExD2S(BaseDim.CabDepth) .. "\n")
  fileW:write("BaseDim.CabHeight = " .. ExD2S(BaseDim.CabHeight) .. "\n")
  fileW:write("BaseDim.CabLength = " .. ExD2S(BaseDim.CabLength) .. "\n")
  fileW:write("BaseDim.FaceFrameBottomRailWidth = " .. ExD2S(BaseDim.FaceFrameBottomRailWidth) .. "\n")
  fileW:write("BaseDim.FaceFrameBottomReveal = " .. ExD2S(BaseDim.FaceFrameBottomReveal) .. "\n")
