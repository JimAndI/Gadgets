function MillingValidater()
  local MyTrue = false

  if Milling.ProfileToolDia == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Default Profile Default Bit Dia - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.PocketToolDia == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Default Pocket Bit Dia - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.DadoBackHeight == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Back Dado Height - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.PartGap == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Part Gap - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.DadoBlindAmount == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Dado Blind Amount - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.DadoHeight == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Dado Height - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.AssemblyHoleStartEnd == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Assembly Hole Start and End Space - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.AssemblyHoleMaxSpace == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Assembly Hole Max Space - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.AssemblyHoleMaxSpace == Milling.AssemblyHoleMinSpace then
    MessageBox("Error: Milling Settings:\n" ..
               "The 'Assembly Hole Max' and 'Assembly Hole Min Space' values cannot equal")
    OnLuaButton_InquiryMilling()

  elseif Milling.AssemblyHoleMaxSpace < Milling.AssemblyHoleMinSpace then
    MessageBox("Error: Milling Settings:\n" ..
               "The 'Assembly Hole Max Space' cannot be larger than 'Assembly Hole Min Space'")
    OnLuaButton_InquiryMilling()

  elseif Milling.AssemblyHoleMinSpace == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Assembly Hole Min Space - value cannot be 0")
    OnLuaButton_InquiryMilling()

  elseif Milling.ShelfPinLen == 0 then
    MessageBox("Error: Milling Settings:\n" ..
               "Shelf Pin Length - value cannot be 0")
    OnLuaButton_InquiryMilling()

  else
    MyTrue = true
  end -- if end
  return MyTrue
end -- if end
