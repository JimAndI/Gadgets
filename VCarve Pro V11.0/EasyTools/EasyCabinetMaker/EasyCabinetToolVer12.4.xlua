function Importer(xFile)
  local xGroup = "Default Settings"
  BaseDim.CabDepth = GetIniValue(xFile, xGroup, "BaseDim.CabDepth", "D")
  BaseDim.CabHeight = GetIniValue(xFile, xGroup, "BaseDim.CabHeight", "D")
  BaseDim.CabLength = GetIniValue(xFile, xGroup, "BaseDim.CabLength", "D")
  BaseDim.FaceFrameBottomRailWidth = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameBottomRailWidth", "D")
  BaseDim.FaceFrameBottomReveal = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameBottomReveal", "D")
  BaseDim.FaceFrameCenterStileWidth = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameCenterStileWidth", "D")
  BaseDim.FaceFrameDrawerHeight1 = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameDrawerHeight1", "D")
  BaseDim.FaceFrameDrawerHeight2 = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameDrawerHeight2", "D")
  BaseDim.FaceFrameDrawerHeight3 = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameDrawerHeight3", "D")
  BaseDim.FaceFrameMidRailWidth = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameMidRailWidth", "D")
  BaseDim.FaceFrameSideReveal = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameSideReveal", "D")
  BaseDim.FaceFrameStileWidth = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameStileWidth", "D")
  BaseDim.FaceFrameThickness = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameThickness", "D")
  BaseDim.FaceFrameTopOverlap = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameTopOverlap", "D")
  BaseDim.FaceFrameTopRailWidth = GetIniValue(xFile, xGroup, "BaseDim.FaceFrameTopRailWidth", "D")
