function MillingValidater()
  local MyTrue = false
  if Milling.ProfileToolDia == 0 then
    MessageBox("Error: Milling Profile Default Tool Dia value cannot be 0")
    OnLuaButton_InquiryMilling()
  elseif Milling.PocketToolDia == 0 then
    MessageBox("Error: Milling Pocket Default Tool Dia value cannot be 0")
    OnLuaButton_InquiryMilling()
  elseif Milling.DadoBackHeight == 0 then
    MessageBox("Error: Milling Dado Back Height value cannot be 0")
    OnLuaButton_InquiryMilling()
  elseif Milling.DadoHeight == 0 then
    MessageBox("Error: Milling Dado Height value cannot be 0")
    OnLuaButton_InquiryMilling()
  elseif Milling.AssemblyHoleStartEnd == 0 then
    MessageBox("Error: Milling Assembly Hole Start and End value cannot be 0")
    OnLuaButton_InquiryMilling()
   elseif Milling.AssemblyHoleMaxSpace  == 0 then
    MessageBox("Error: Milling Assembly Hole Max Space value cannot be 0")
    OnLuaButton_InquiryMilling()
  elseif Milling.AssemblyHoleMinSpace  == 0 then
    MessageBox("Error: Milling Assembly Hole Min Space value cannot be 0")
    OnLuaButton_InquiryMilling()
  elseif Milling.ShelfPinLen == 0 then
    MessageBox("Error: Milling Shelf Pin Length value cannot be 0")
    OnLuaButton_InquiryMilling()
  else
    MyTrue = true
  end -- if end
  return MyTrue
