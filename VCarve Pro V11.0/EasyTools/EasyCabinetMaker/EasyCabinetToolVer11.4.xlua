  BaseDim.FaceFrameDrawerHeight3            = RegistryRead:GetDouble("BaseDim.FaceFrameDrawerHeight3",          6.0000)
  BaseDim.FaceFrameMidRailWidth             = RegistryRead:GetDouble("BaseDim.FaceFrameMidRailWidth",           1.5000)
