function NameValidater(FileName)
  local MyTrue = true
  local strlen = string.len(FileName)
  local strup = string.upper(FileName)
  local i = 1
  local y = ""
  while i <=  strlen do
    y = string.byte(string.sub(strup, i, i))
    if y == 32 then
      MyTrue = true
    elseif y == 45 then
      MyTrue = true
    elseif (y >= 48) and (y <= 57) then
      MyTrue = true
    elseif (y >= 65) and (y <= 90) then
      MyTrue = true
    else
      MyTrue = false
      break
    end -- if end
    i = i + 1
  end -- while end;
  return MyTrue
end -- if end
